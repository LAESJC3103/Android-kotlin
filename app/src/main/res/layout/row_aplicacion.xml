<?xml version="1.0" encoding="utf-8"?>

<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardViewItemAplicacion"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/activity_vertical_margin"
    android:layout_marginTop="@dimen/item_aplication_margin_top_bottom"
    android:layout_marginEnd="@dimen/activity_vertical_margin"
    android:layout_marginBottom="@dimen/item_aplication_margin_top_bottom"
    app:cardCornerRadius="@dimen/item_aplicacion_corner_radius">

    <include
        android:id="@+id/constraintLayoutAplicacion"
        layout="@layout/content_row_aplicacion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</android.support.v7.widget.CardView>
